{
    "h1": {
        "prefix": "h1",
        "body": ["# $0"],
        "description": "Heading 1"
    },
    "h2": {
        "prefix": "h2",
        "body": ["## $0"],
        "description": "Heading 2"
    },
    "h3": {
        "prefix": "h3",
        "body": ["### $0"],
        "description": "Heading 3"
    },
    "h4": {
        "prefix": "h4",
        "body": ["#### $0"],
        "description": "Heading 4"
    },
    "h5": {
        "prefix": "h5",
        "body": ["##### $0"],
        "description": "Heading 5"
    },
    "italic": {
        "prefix": "italic",
        "body": ["*$0*"],
        "description": "Italic text"
    },
    "bold": {
        "prefix": "bold",
        "body": ["**$0**"],
        "description": "Bold text"
    },
    "highlight": {
        "prefix": "highlight",
        "body": ["==$0=="],
        "description": "Highlight"
    },

    "codeblock": {
        "prefix": "codeblock",
        "body": [
            "```$1",
            "$0",
            "```"
        ],
        "description": "Code block with language"
    },
    "frontmatter": {
        "prefix": "#front",
        "body": [
            "+++",
            "title = '$1'",
            "tags = [$2]",
            "date = ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
            "description = '$3'",
            "+++",
            "",
            "# $4",
            ""
        ],
        "description": "Markdown frontmatter block"
    },
    "journaling": {
        "prefix": "#journaling",
        "body": [
            "# ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
            "",
            "## Today",
            "## Rant",
            "## Tommorow"
        ],
        "description": "Journaling thing"
    }
}
